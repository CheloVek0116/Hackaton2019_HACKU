{
    "manifest_version": 2,
    "content_scripts": [{"matches": ["<all_urls>","*://*/*"],"js": ["jquery-3.4.1.min.js", "background.js"]}],
    "name": "plugin",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "tabs",
        "identity",
        "*://localhost:8000/*",

        "webRequest",
        "webRequestBlocking"
    ],

    "background": {
        "scripts":["jquery-3.4.1.min.js", "background.js"]
    }
}